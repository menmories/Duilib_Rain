<?xml version="1.0" encoding="UTF-8"?>
<Window size="1200,720" caption="0,0,0,30" sizebox="8,8,8,8">
	<VerticalLayout>
		<!--用来做标题栏 高度固定30pix,宽度随着窗口的宽度变化 bkcolor="#ff1378fc"-->
		<HorizontalLayout height="30" bkcolor="#ff333433">
			<HorizontalLayout></HorizontalLayout>
			<HorizontalLayout width="120">
				<Button name="btn_min" text="-" normalimage="XML\btn_caption_normal.png" hotimage="XML\btn_caption_hot.png" pushedimage="XML\btn_caption_pushed.png"/>
				<Button name="btn_max" text="口" normalimage="XML\btn_caption_normal.png" hotimage="XML\btn_caption_hot.png" pushedimage="XML\btn_caption_pushed.png"/>
				<Button name="btn_close" text="X" normalimage="XML\btn_caption_normal.png" hotimage="XML\btn_caption_close_hot.png" pushedimage="XML\btn_caption_close_pushed.png"/>
			</HorizontalLayout>
		</HorizontalLayout>
		<!--如果不设定宽和高则宽和高会自动扩展至最大-->
		<!-- <VerticalLayout bkimage="xml\psc.jpg"> -->
		<VerticalLayout bkimage="xml\psc.jpg">
			<HorizontalLayout height="128">
				<VerticalLayout width="128" bkimage="xml\20230719225947.jpg"/>
				<VerticalLayout width="128" bkimage="xml\20230719230009.jpg"/>
				<VerticalLayout width="128" bkimage="xml\20230719225947.jpg"/>
				<VerticalLayout width="128" bkimage="xml\20230719230009.jpg"/>
				<VerticalLayout width="128" bkimage="xml\20230719225947.jpg"/>
			</HorizontalLayout>
		</VerticalLayout>
	</VerticalLayout>
</Window>
